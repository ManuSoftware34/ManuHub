local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("ManuHub", "BloodTheme") 
local Tab = Window:NewTab("Home") 
local Section = Tab:NewSection("Player") 
Section:NewSlider("Walk Speed", "Set Walk Speed", 500, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end) 
Section:NewSlider("Jump Power", "Set Jump Power", 500, 50, function(v) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
end) 
Section:NewKeybind("Show script", "Toggle script", Enum.KeyCode.LeftAlt, function()
	Library:ToggleUI()
end) 
Section:NewButton("XXHub (FE)", "Load XXHub", function()
    loadstring(game:HttpGet(('https://pastebin.com/raw/YVE4njap'),true))()
end)
